{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--package",
				"s7-client",
				"--example",
				"cotp_connect",
				"--verbose"
			],
			"problemMatcher": [
				"$rustc",
				"$rust-panic"
			],
			"group": "build",
			"label": "rust: run cotp_connect"
		}
	]
}